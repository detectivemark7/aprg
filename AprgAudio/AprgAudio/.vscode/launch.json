{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run In WSL",
            "type": "node",
            "request": "launch",
            "program": "./.vscode/runWslCommonScript.sh",
            "args": [
                "outputWithHighlighting",
                "${workspaceFolderBasename}",
                "runFileWithProjectName"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "Build In WSL Using Clang With Asan",
            "runtimeExecutable": "wsl",
            "runtimeArgs": [
                "bash"
            ],
            "env": {},
            "stopOnEntry": true,
            "outFiles": [],
            "sourceMaps": false,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Debug locally with MSVC",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../VisualStudioBuild/build/visualstudio-windows-msvc-debug/${workspaceFolderBasename}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal"
        }
    ]
}